{   
    "defaultSeverity": "error",
    "parserOptions": {
      "ecmaVersion": 2021
    },
    "extends": [
      "tslint:recommended",
      "prettier"
    ],
    "jsRules": {},
    "env": {
      "es6": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "no-console":  [true, "warning"],
      "trailing-comma": [ false ],
      "no-empty": [ false ],
      "jsdoc-format": false
    },
     
    "rulesDirectory": []
  }